<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="475.0" prefWidth="547.0" spacing="10.0" style="-fx-padding: 10;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lp.JavaFxClient.controllers.TypesController">
   <children>
      <HBox alignment="CENTER" prefHeight="91.0" spacing="30.0">
         <children>
            <Label prefHeight="30.0" prefWidth="69.0" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Types" />
            <Pane prefHeight="281.0" prefWidth="217.0" />
            <Button onAction="#onRefresh" prefHeight="25.0" prefWidth="61.0" text="Refresh" />
            <Button mnemonicParsing="false" onAction="#onAddType" prefHeight="25.0" prefWidth="46.0" text="Add" />
            <Button mnemonicParsing="false" onAction="#onEditType" prefHeight="25.0" prefWidth="46.0" text="Edit" />
         </children>
      </HBox>
      <TableView fx:id="tableTypes" prefHeight="353.0" prefWidth="517.0">
        <columns>
          <TableColumn fx:id="idType" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="nameType" prefWidth="75.0" text="Type" />
        </columns>
      </TableView>
   </children>
</VBox>
